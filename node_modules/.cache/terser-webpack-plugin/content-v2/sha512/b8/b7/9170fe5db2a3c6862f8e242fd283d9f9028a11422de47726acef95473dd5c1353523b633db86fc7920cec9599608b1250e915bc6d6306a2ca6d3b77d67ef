st OrderListView = () => {\r\n\r\n    // Table setup\r\n    const [DATA, setDATA] = useState([])\r\n    const {\r\n        getTableProps,\r\n        getTableBodyProps,\r\n        headerGroups,\r\n        rows,\r\n        prepareRow\r\n    } = useTable({\r\n        columns: COLUMNS,\r\n        data: DATA\r\n    })\r\n\r\n    // refreshTable\r\n    const refreshTable = () => {\r\n        console.log('refresh function')\r\n        axios({\r\n            method: 'get',\r\n            url: `${REACT_APP_API_BASE_URL}management/sizing`\r\n        })\r\n            .then(res => {changes = {}; setDATA([]); setDATA(res.data)})\r\n            .catch(err => console.error(err));\r\n    }\r\n\r\n    useEffect(refreshTable, [])\r\n\r\n    // Post sizes\r\n    \r\n    const [loading, setLoading] = useState([false, '', ''])\r\n\r\n    const submit = () => {\r\n        setLoading([true, 'Sending data to server...', 'spinner']);\r\n\r\n        // filter data and add user info\r\n        const filtered = {};\r\n        for (const id in changes) {\r\n            if(changes[id] !== ''){\r\n                filtered[id] = changes[id]\r\n            }\r\n        }\r\n        filtered['user'] = 'testuser'\r\n        console.log(filtered)\r\n        // sending data to server. - events will be created and event ids will return\r\n        axios({\r\n            method: 'post',\r\n            url: `${REACT_APP_API_BASE_URL}management/sizing/`,\r\n            data: filtered,\r\n        })\r\n            .then(res => {\r\n                console.log(res.data);\r\n                const errors = res.data.filter(obj => obj.result !== 'OK');\r\n                console.log(errors)\r\n                if(errors.length > 0) {\r\n\r\n                    setLoading([true, `Error occured for some orders: ${errors}`, 'error']);\r\n                    wait(2*1000).then(() => {\r\n                        setLoading([false, '', '']);\r\n                        refreshTable(); })\r\n                }else {\r\n                    setLoading([true, 'Successfull.', 'done']);\r\n                    wait(2*1000).then(() => {\r\n                        setLoading([false, '', '']);\r\n                        refreshTable(); })\r\n                }\r\n            })\r\n            .catch(err => {\r\n                console.log(err);\r\n                setLoading([true, '! SERVER ERROR !', 'error']);\r\n                wait(2*1000).then(() => {\r\n                    setLoading([false, '', '']);\r\n                    refreshTable(); })\r\n            })\r\n    }\r\n\r\n    return (\r\n        <>\r\n            {loading[0] && <Loading message={loading[1]} symbol={loading[2]}/>}\r\n            <Div>\r\n                <TableDiv>\r\n                    <Table {...getTableProps}>\r\n                        <thead>\r\n                            {headerGroups.map((headerGroup) => (\r\n                                    <tr {...headerGroup.getHeaderGroupProps()}>\r\n                                        {headerGroup.headers.map( column => (\r\n                                            <th {...column.getHeaderProps()}>{column.render('Header')}</th>\r\n                                        ))}\r\n                                    </tr>\r\n                                ))}\r\n                        </thead>\r\n                        <tbody {...getTableBodyProps}>\r\n                            {rows.map((row) => {\r\n                                prepareRow(row)\r\n                                return <tr {...row.getRowProps()}>\r\n                                    {row.cells.map( cell => (\r\n                                        <td {...cell.getCellProps}>{cell.render('Cell')}</td>\r\n                                    ))}\r\n                                </tr>\r\n                            })}\r\n                        </tbody>\r\n                    </Table>\r\n                </TableDiv>\r\n                <BottomDiv><Button onClick={submit}>Submit</Button></BottomDiv>\r\n            </Div>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default OrderListView","import styled from 'styled-components'\r\nimport OrderListView from './OrderListView'\r\n\r\nconst Div = styled.div`\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    align-items: center;\r\n    height: 95%;\r\n`;\r\n\r\nconst Sizing = () => {\r\n\r\n    return (\r\n        <Div>\r\n            <OrderListView page='Sizing' />\r\n        </Div>\r\n    )\r\n}\r\n\r\nexport default Sizing","// import styled from 'styled-components'\r\n\r\n\r\nconst OnHold = () => {\r\n    return (\r\n        <div>\r\n            OnHold\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default OnHold","// import styled from 'styled-components'\r\n\r\n\r\nconst Archive = () => {\r\n    return (\r\n        <div>\r\n            Archive\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Archive","import styled from 'styled-components'\r\nimport Sizing from './Sizing'\r\nimport OnHold from './OnHold'\r\nimport Archive from './Archive'\r\n\r\nconst Div = styled.div`\r\n    border-style: solid;\r\n    height: 97%;\r\n    overflow: auto;\r\n`;\r\n\r\nconst Content = ({ activePage, accessrights }) => {\r\n    return (\r\n        <Div>\r\n            {(activePage === 'Sizing' && accessrights.includes('Manage | Sizing')) && <Sizing />}\r\n            {(activePage === 'On Hold' && accessrights.includes('Manage | On Hold')) && <OnHold />}\r\n            {(activePage === 'Archive' && accessrights.includes('Manage | Archive')) && <Archive />}\r\n        </Div>\r\n    )\r\n}\r\n\r\nexport default Content","import { useState, useEffect, useRef } from 'react'\r\nimport { useHistory } from 'react-router-dom'\r\nimport styled from 'styled-components'\r\nimport Navbar from './Navbar'\r\nimport Header from './Header'\r\nimport Content from './content/Content'\r\nimport { checkAccessRights } from '../../global/Services'\r\n\r\nconst Div = styled.div`\r\n    display: grid;\r\n    width: 100%;\r\n    grid-template-columns: 90%;\r\n    grid-template-rows: 1fr 1fr 25fr;\r\n    grid-gap: 0.1em;\r\n    justify-content: center;\r\n    height:100%;\r\n`;\r\n\r\nconst Manage = () => {\r\n    const [accessrights, setAccessrights] = useState([]);\r\n    const history = useRef();\r\n    history.current = useHistory();\r\n\r\n    useEffect(() => {\r\n        checkAccessRights().then(access => {\r\n            if(access) {\r\n                if(access.includes('Modules | Manage')){\r\n                    setAccessrights(access);\r\n                }else{\r\n                    history.current.push('/menu');\r\n                }\r\n            }else{\r\n                history.current.push('/login');\r\n            }\r\n        })}, [])\r\n\r\n    const [activePage, setActivePage] = useState('On Hold');\r\n\r\n    return (\r\n        <Div>\r\n            <Header />\r\n            <Navbar setActivePage={setActivePage} accessrights={accessrights}/>\r\n            <Content activePage={activePage} accessrights={accessrights}/>\r\n        </Div>\r\n    )\r\n}\r\n\r\nexport default Manage","import { useState, useRef, useEffect } from 'react'\r\nimport { Link, useHistory } from 'react-router-dom'\r\nimport { checkAccessRights } from '../../global/Services'\r\n\r\nconst Dashboard = () => {\r\n    const [accessrights, setAccessrights] = useState([]);\r\n    const history = useRef();\r\n    history.current = useHistory();\r\n\r\n    useEffect(() => {\r\n        checkAccessRights().then(access => {\r\n            if(access) {\r\n                if(access.includes('Modules | Dashboard')){\r\n                    setAccessrights(access);\r\n                }else{\r\n                    history.current.push('/menu');\r\n                }\r\n            }else{\r\n                history.current.push('/login');\r\n            }\r\n        })}, [])\r\n\r\n    return (\r\n        <div>\r\n            <h1>Dashboard</h1>\r\n            <Link to='/menu'><button>Menu</b