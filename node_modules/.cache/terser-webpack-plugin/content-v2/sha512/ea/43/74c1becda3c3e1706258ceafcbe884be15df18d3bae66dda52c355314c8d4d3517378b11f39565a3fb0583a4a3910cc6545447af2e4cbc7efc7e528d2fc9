       }else{\r\n                setLoading([true, '! SERVER ERROR !', 'error']);\r\n                setTimeout(() => {\r\n                    setLoading([false, '', '']);\r\n                    refreshTable();\r\n                }, 1000)\r\n            }\r\n        })\r\n    }\r\n\r\n    return (\r\n        <Outerdiv>\r\n            {loading[0] && <Loading message={loading[1]} symbol={loading[2]}/>}\r\n            <Div>\r\n                <Table data={DATA} columns={COLUMNS}/>\r\n                <BottomDiv><Button onClick={submit}>Submit</Button></BottomDiv>\r\n            </Div>\r\n        </Outerdiv>\r\n    )\r\n}\r\n\r\nexport default Sizing","// import styled from 'styled-components'\r\n\r\n\r\nconst OnHold = () => {\r\n    return (\r\n        <div>\r\n            OnHold\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default OnHold","// import styled from 'styled-components'\r\n\r\n\r\nconst Archive = () => {\r\n    return (\r\n        <div>\r\n            Archive\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default Archive","import styled from 'styled-components'\r\nimport Sizing from './Sizing'\r\nimport OnHold from './OnHold'\r\nimport Archive from './Archive'\r\n\r\nconst Div = styled.div`\r\n    border-style: solid;\r\n    height: 97%;\r\n    overflow: auto;\r\n`;\r\n\r\nconst Content = ({ activePage, accessrights }) => {\r\n    return (\r\n        <Div>\r\n            {(activePage === 'Sizing' && accessrights.includes('Manage | Sizing')) && <Sizing />}\r\n            {(activePage === 'On Hold' && accessrights.includes('Manage | On Hold')) && <OnHold />}\r\n            {(activePage === 'Archive' && accessrights.includes('Manage | Archive')) && <Archive />}\r\n        </Div>\r\n    )\r\n}\r\n\r\nexport default Content","import { useState, useEffect, useRef } from 'react'\r\nimport { useHistory } from 'react-router-dom'\r\nimport styled from 'styled-components'\r\nimport Navbar from './Navbar'\r\nimport Header from './Header'\r\nimport Content from './conten